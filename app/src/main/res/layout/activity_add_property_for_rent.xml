                android:orientation="horizontal"
                android:weightSum="2"
                android:background="#EDF0F2"
                >

                <TextView
                    android:id="@+id/fr_price_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/amount_payable_per_month"
                    style="@style/CustomTextStyle14"
                    android:textColor="@color/black"
                    android:layout_weight="1.3"
                    />

                <EditText
                    android:id="@+id/fr_price_et"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/under_line_textview_light"
                    android:inputType="number"
                    android:maxLength="5"
                    style="@style/CustomTextStyle18"
                    android:layout_marginEnd="@dimen/addPropertyStartAndEnd"
                    android:padding="@dimen/textViewPadding"
                    android:layout_weight="0.7"
                    android:drawableStart="@drawable/ic_rupee"
                    android:textColor="@color/lightGreen"
                    android:drawablePadding="7dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    tools:ignore="LabelFor" />

            </LinearLayout>



            <!--bedroom text-->
    <TextView
        android:id="@+id/fr_bedroom_tv"
