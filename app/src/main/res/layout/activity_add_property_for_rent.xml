<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".AddPropertyForRent">

    <android.support.v7.widget.Toolbar
        android:id="@+id/fr_toolbar"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@color/appColor"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!--back button-->
            <ImageButton
                android:id="@+id/fr_back_ib"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:background="@drawable/ic_return_back_white_smoke"
                tools:ignore="ContentDescription" />

            <!--edit profile-->
            <TextView
                android:layout_toEndOf="@+id/fr_back_ib"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_property"
                style="@style/CustomTextStyle14"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:paddingStart="@dimen/activity_horizontal_margin"
                tools:ignore="RelativeOverlap,RtlSymmetry" />

            <!--loading view-->
            <com.github.ybq.android.spinkit.SpinKitView
                android:visibility="gone"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/fr_spin_kit"
                style="@style/SpinKitView.Small.Circle"
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/activity_horizontal_margin"
                app:SpinKit_Color="@color/white" />

        </RelativeLayout>

    </android.support.v7.widget.Toolbar>




    <TextView
        android:id="@+id/fr_select_image"
        android:layout_below="@+id/fr_toolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/select_images"
        style="@style/CustomTextStyle10"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:textAllCaps="true"
        android:textColor="@color/gray"
        />

    <LinearLayout
