                />


            <com.jaredrummler.materialspinner.MaterialSpinner
                android:layout_below="@+id/fr_furnished_tv"
                android:layout_marginStart="@dimen/addPropertyStartAndEnd"
                android:layout_marginEnd="@dimen/addPropertyStartAndEnd"
                android:id="@+id/fr_furnished_s"
                android:layout_width="match_parent"
                android:layout_marginTop="4dp"
                app:ms_background_color="@color/lightGreen"
                style="@style/CustomTextStyle14"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:layout_height="wrap_content"/>



            <!--bachelors-->
            <TextView
                android:id="@+id/fr_bachelor_tv"
                android:layout_below="@+id/fr_furnished_s"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bachelors allowed"
                android:textAllCaps="true"
                style="@style/CustomTextStyle12"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginStart="@dimen/addPropertyStartAndEnd"
                android:textColor="@color/gray"
                android:drawableEnd="@drawable/ic_placeholder_profile_pic"
                android:gravity="center_vertical"
                android:drawablePadding="7dp"
                />
            <co.ceryle.radiorealbutton.RadioRealButtonGroup
                android:id="@+id/fr_radio_bachelor"
                android:layout_below="@+id/fr_bachelor_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                app:rrbg_animateDrawables_enter="overshoot"
                app:rrbg_animateTexts_enter="overshoot"
                app:rrbg_dividerColor="@color/black"
                app:rrbg_dividerSize="1dp"
                app:rrbg_radius="10dp"
                app:rrbg_selectorColor="@color/lightGreen"
                app:rrbg_selectorSize="6dp"
                android:layout_marginStart="@dimen/addPropertyStartAndEnd"
                android:layout_marginEnd="@dimen/addPropertyStartAndEnd"
                >

                <co.ceryle.radiorealbutton.RadioRealButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:rrb_drawablePadding="8dp"
                    app:rrb_ripple="true"
                    app:rrb_rippleColor="@color/black50"
                    app:rrb_text="Yes"
                    app:rrb_drawableGravity="right"
                    app:rrb_textColor="@color/black"/>

                <co.ceryle.radiorealbutton.RadioRealButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:rrb_drawableGravity="right"
                    app:rrb_drawablePadding="8dp"
                    app:rrb_ripple="true"
                    app:rrb_rippleColor="@color/black50"
                    app:rrb_text="No"
                    app:rrb_textColor="@color/black"/>
            </co.ceryle.radiorealbutton.RadioRealButtonGroup>


        </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>

    <Button
        android:id="@+id/fr_submit_b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/submit"
        android:background="@color/appColor"
        android:layout_alignParentBottom="true"
        />

</RelativeLayout>